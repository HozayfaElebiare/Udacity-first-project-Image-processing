{
    "name": "morgan",
    "version": "1.0.0",
    "description": "udacity project 1 is an image processing api that let users resize and reshape images that saved in our server in save it inside sirverside folder and return this image ",
    "main": "build/index.js",
    "scripts": {
        "dev": "nodemon  --exec ts-node ./src/index.ts",
        "build": "npx tsc",
        "start:prod": "npm run build && node build/index.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix",
        "format": "prettier --write src/**/*.ts",
        "jasmine": "jasmine",
        "test": "npm run build && npm run jasmine",
        "prettier": "prettier --config .prettierrc {,!(node_modules)/**/}*.ts --write"
    },
    "keywords": [
        "node",
        "nodemon",
        "udacity",
        "mahmoud",
        "elebiare",
        "project1",
        "api",
        "image",
        "processing"
    ],
    "author": "Mahmoud Elebiare",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jasmine": "^3.10.3",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.10",
        "@types/sharp": "^0.29.5",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^4.0.2",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.15",
        "prettier": "2.5.1",
        "supertest": "^6.2.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "dotenv": "^14.2.0",
        "express": "^4.17.2",
        "morgan": "^1.10.0",
        "sharp": "^0.29.3"
    }
}